{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "experimentalDecorators": true,
    "jsx": "react",
    "lib": ["es6"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "baseUrl": "src",
    "inlineSourceMap": true,
    "paths": {
      "components/*": ["./components/*"],
      "app/*": ["./app/*"],
      "icons/*": ["./icons/*"],
      "img/*": ["./img/*"],
      "views/*": ["./views/*"],
      "components": ["./components"],
      "app": ["./app"],
      "icons": ["./icons"],
      "img": ["./img"],
      "views": ["./views"],
    }
  },
  "exclude": ["node_modules", "babel.config.js", "metro.config.js", "jest.config.js", "**/*.test.ts", "**/*.test.tsx"]
}
